{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-alert": 0,
        "no-console": 0,
        "no-unused-vars": 0,
        "consistent-return" : 0,
        "react/no-access-state-in-setstate": 0,
        "react/destructuring-assignment":0,
        "react/react-in-jsx-scope":0,
        "react/jsx-filename-extension":0,
        "camelcase": 0,
        "max-len": 0
    }
}
